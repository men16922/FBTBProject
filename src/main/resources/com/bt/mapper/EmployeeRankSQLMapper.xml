<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bt.mapper.EmployeeRankSQLMapper">

	
	<select id="selectByRankNo" resultType="com.bt.vo.EmployeeRankVo">
	<![CDATA[
		SELECT *
 		FROM Employee_rank
 		WHERE rank_no = #{rank_no}
		
	]]>
	</select>


	
</mapper>
